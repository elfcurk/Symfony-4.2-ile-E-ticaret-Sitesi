
    <h1>Galeriye yeni görsel ekle</h1>

    {{ include('admin/image/_form.html.twig') }}

{% for image in imagelist %}
    <img src="{{ asset ('uploads') }}/images/{{ image.image }}" height="300">

    <a href="{{ path('admin_image_del',{'id':image.id,'pid':image.productid}) }}" onclick="return confirm('Silmek istediğine emin misin?');" class="btn btn-1 btn-danger">Sil</a>
{% endfor %}